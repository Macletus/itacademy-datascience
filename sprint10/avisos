	function aceptaCookies() 
	{ 
		 $.cookie("accept_cookies", "ok", { expires: 1095 ,path: '/' });
	}
	
	var cookie_aemet = $.cookie("accept_cookies");
	if(cookie_aemet == null)
	{
		 $("#popup_cookies").show();
