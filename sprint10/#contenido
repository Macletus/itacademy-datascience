<script type="text/javascript">
//<![CDATA[

$(function() {
 
  var lineas = $("#lineas-de-interes");
  
  lineas.owlCarousel({
      items : 8, //10 items above 1000px browser width
      itemsDesktop : [1000,6], //5 items between 1000px and 901px
      itemsDesktopSmall : [900,5], // betweem 900px and 601px
      itemsTablet: [600,4], //2 items between 600 and 0
      itemsMobile : false // itemsMobile disabled - inherit from itemsTablet option
  });

  var productos = $("#productos-destacados");
  
  productos.owlCarousel({
      items : 4, //10 items above 1000px browser width
      itemsDesktop : [1000,4], //5 items between 1000px and 901px
      itemsDesktopSmall : [900,3], // betweem 900px and 601px
      itemsTablet: [600,3], //2 items between 600 and 0
      itemsMobile : false // itemsMobile disabled - inherit from itemsTablet option
  });
  
  var noticias = $("#carrusel-noticias");
  
  noticias.owlCarousel({
	  items : 1,
      singleItem: true,
      itemsDesktop : [1000,1],
      itemsDesktopSmall : [900,1],
      itemsTablet: [600,1],
      itemsMobile: [479,1],
      autoPlay : true,
      beforeInit: function($elem) {
          var length = $elem.children().length;
          var looplength = this.options.items - length;
          if(looplength > 0) {
            for(var i=0;i<looplength;i++) {
              $elem.append('<div></div>');
            }
          }
        }
  });  
});

var ambitoPredSeleccionado1 = $('ul#tabs-mapa-prediccion > li.ambito-selected').attr('data-mostrar-datos-pr');

function mandarAmbito(){
	var ambito = "";
	document.form.i.value=ambito;
	document.form.submit();
	}

	

var ult_mun_visit = "no_mun";
var cookie_aemet = $.cookie("ultimas_localidades");

$("#fila_imp_loc_visit").empty();

if(cookie_aemet != null)
{
	 ult_mun_visit = "";
	 n_ult_mun_visit = 4;
	 var ult_localidades = cookie_aemet.split("#");
	 for (x=0;x < ult_localidades.length - 1 && x < n_ult_mun_visit;x++)
	 {
		if( x == (ult_localidades.length - 1))
		{
			 var localidad = ult_localidades[x].split("@");
			 ult_mun_visit += localidad[1];
		}
		else
		{
			 var localidad = ult_localidades[x].split("@");
			 ult_mun_visit += localidad[1] + ",";
		}

	 }
}

var removeLastChar = function(value, character){ 
	var lastChar = value.slice(-1); 
	if(lastChar == character) { 
		value = value.slice(0, -1); 
	} 
	return value; 
} 

var result = removeLastChar(ult_mun_visit, ','); 
 
ult_mun_visit = result;

var mun_geoloc = "";

	var geo = null;
	//funcion encargada de llamar a la pagina y pasar los parametros de latitud y longitud
	function displayPosition(position) {
		$.ajax({			
			  url: "/es/eltiempo/prediccion/municipios/geolocalizacion?munhome=" + ult_mun_visit + "&y=" + position.coords.latitude + "&x=" + position.coords.longitude,
			  success: function(data){	
				$("#fila_imp_loc_visit").empty();
			    $("#fila_imp_loc_visit").append(data);
			    $("#cont_loc_visitadas").show();
// 			    $( "#cont_loc_visitadas" ).slideDown("slow");

				if(ult_mun_visit == "no_mun"){
				    $(".listado_municipios_portada").removeClass('elem0');
				    $(".listado_municipios_portada").addClass('elem1');
				} else {
					contarMunicipiosVisitados();
				}
			    mun_geoloc = "mun_geolocalizado";
			  }
			});
	}
	//En caso de error en getCurrentPosition no hacemos nada
	function displayError(positionError) {
		if(ult_mun_visit != "no_mun")
		{
			$.ajax({
 				  url: "/es/eltiempo/prediccion/municipios/geolocalizacion?munhome=" + ult_mun_visit,
				  success: function(data){
					$("#fila_imp_loc_visit").empty();
				    $("#fila_imp_loc_visit").append(data);
				    $("#cont_loc_visitadas").show();
//				    $( "#cont_loc_visitadas" ).slideDown("slow");
				  }
				});
		}
	}

	try 
	{	//creamos la objeto para la gestion de geolocation
		if(typeof(navigator.geolocation) == 'undefined')
			geo = google.gears.factory.create('beta.geolocation');
		else 
			geo = navigator.geolocation;
		
		//mostramos las ultimas visitadas
		if(ult_mun_visit != "no_mun"){
				$.ajax({
	 				  url: "/es/eltiempo/prediccion/municipios/geolocalizacion?munhome=" + ult_mun_visit,
					  success: function(data){
						  
						$("#fila_imp_loc_visit").empty();
					    $("#fila_imp_loc_visit").append(data);
					    $("#cont_loc_visitadas").show();
//	 				    $( "#cont_loc_visitadas" ).slideDown("slow");	

						contarMunicipiosVisitados();
					  }
					});
	    }else{
	    	$(".listado_municipios_portada").addClass('elem0');
	    }
		
		
		//Si se ha creado correctamente el objeto de geolocation llamamos a la funcion, sino no hacemos nada
		if (geo) 
			geo.getCurrentPosition(displayPosition, displayError);
		else
			geo_position_js.getCurrentPosition(displayPosition, displayError);
	}catch(e){}	
	
	function contarMunicipiosVisitados(){
		var elem_listado_municipios = $("#fila_imp_loc_visit li");
	    var num_elem_listado_municipios = $(".listado_municipios_portada li").size();
	    
	    var mun_geolocal = $(".listado_municipios_portada li.municipio_geolocalizado");
	    $(".texto_filtro_municipio").css('display','none');
    	switch (num_elem_listado_municipios) {
    		
	      case 0:
	        $(".listado_municipios_portada").addClass('elem0');
	        $(".texto_filtro_municipio").css('display','block');
	        break;
	      case 1:
	        $(".listado_municipios_portada").removeClass('elem0');
	        $(".listado_municipios_portada").addClass('elem1');
	        $(".texto_filtro_municipio").css('display','block');
	        break;
	      case 2:
	        $(".listado_municipios_portada").removeClass('elem0');
	        $(".listado_municipios_portada").removeClass('elem1');
	        $(".listado_municipios_portada").addClass('elem2');
	        $(".texto_filtro_municipio").css('display','block');
	        break;
	      default:
	    	$(".listado_municipios_portada").removeClass('elem2');
	    }
	}
	
	$(document).ready(function(){
		$('#url-ver-prediccion').attr('href','/es/eltiempo/prediccion/espana');
	    //$('.ico_fondo_cuadrado .listado_avisos_cabecera').remove();
	    
	    $('#url-ver-prediccion').on('click',function(){
	    	
	    	var $ambitoMapa = $('#tabs-mapa-prediccion').find('li.ambito-selected').attr('data-mostrar-datos-pr').toLowerCase();
	    	var href = $(this).attr('href');
	    	var indexHref = href.indexOf("?");
	    	href = (indexHref != -1) ? href.substring(0, indexHref) : href;
	    	$(this).attr('href', href + "?a=" + $ambitoMapa);
	    });
	    
 	    //(var mapaPred = document.getElementById("url-ver-prediccion-mapa");
 	    $('#url-ver-prediccion-mapa').on('click',function(){ 	    	
 	    	var $ambitoMapaPred = $('#tabs-mapa-prediccion').find('li.ambito-selected').attr('data-mostrar-datos-pr').toLowerCase();
 	    	var hrefMapa = $(this).attr('href'); 	    	
 	    	var indexHrefMapa = hrefMapa.indexOf("?"); 	    	
 	    	hrefMapa = (indexHrefMapa != -1) ? hrefMapa.substring(0, indexHrefMapa) : hrefMapa; 	    	
 	    	$(this).attr('href', hrefMapa + "?a=" + $ambitoMapaPred); 	    	
 	    });
	    //Boton Ver mas
	    $('#url-ver-avisos').on('click',function(){
	    	var $ambitoMapaAvisos = $('#tabs-mapa-avisos').find('li.ambito-selected').attr('data-mostrar-datos-av').toLowerCase();
	    	var hrefAvisos = $(this).attr('href');
	    	$(this).attr('href', hrefAvisos + "&a=" + $ambitoMapaAvisos);
	    });	    

	    //Botones avisos dia
	    $('.sin_aviso_esp_listado_avisos_portada li').on('click','a',function(){
 		  var ambitoMapaAvisosDia = $('#tabs-mapa-avisos').find('li.ambito-selected').attr('data-mostrar-datos-av').toLowerCase();
	    	var hrefAvisosDia = $(this).attr('href');
	    	var str = hrefAvisosDia.split("?");
	    	hrefAvisosDia = str[0] + "?" + str[1];
			$(this).attr('href', hrefAvisosDia + "&a=" + ambitoMapaAvisosDia + "&r=1");
 	   });
 	    
		//Mapa de avisos
 	    $('#url-ver-avisos-mapa').on('click',function(){ 	    	
 	    	var $ambitoMapaPred = $('#tabs-mapa-avisos').find('li.ambito-selected').attr('data-mostrar-datos-av').toLowerCase();
 	    	var hrefMapa = $(this).attr('href'); 	    	
 	    	$(this).attr('href', hrefMapa + "&a=" + $ambitoMapaPred); 	    	
 	    });		
	 
	});

 	// Radar
    $('#url-ver-radar-mapa,#link-radar').each(function(){
    	$(this).on('click',function(){
        	var $ambitoMapaPred = $('#tabs-mapa-radar').find('li.ambito-selected').attr('data-mostrar-datos-rd').toLowerCase();
        	if($ambitoMapaPred == 'can'){
        		$(this).attr('href', $(this).attr('href') + "?p=ca");	
        	} 	    	
    	 });
    });

 	    
	//Check to see if the window is top if not then display button
	$(window).scroll(function(){
		if ($(this).scrollTop() > 100) {
			$('.scrollToTop').fadeIn();
		} else {
			$('.scrollToTop').fadeOut();
		}
	});
	
	//Click event to scroll to top
	$('.scrollToTop').click(function(){
		$('html, body').animate({scrollTop : 0},800);
		return false;
	});
	
	//]]>	
</script>
